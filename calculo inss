salario = float(input())
teto = 5839.45

if (salario<= 1751.81):

    print("Desconto do INSS: R$ {:.2f}".format (salario*8/100))
    
elif (salario>=1751.81 and salario<=2919.72):

    print("Desconto do INSS: R$ {:.2f}".format (salario*9/100))
    
elif (salario>=2919.72 and salario<=5839.45):

    print("Desconto do INSS: R$ {:.2f}".format (salario*11/100))
    
    
elif (salario> teto):

    print ("Desconto do INSS: R$ {:.2f}".format ( teto*11/100))
